{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./src"],
  "entryPointStrategy": "expand",
  "out": "../docs/api/typescript",
  "exclude": [
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/node_modules/**"
  ],
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeInternal": true,
  "includeVersion": true,
  "readme": "none",
  "name": "Aura Video Studio Web API",
  "plugin": [],
  "sort": ["kind", "instance-first", "alphabetical"],
  "kindSortOrder": [
    "Interface",
    "Class",
    "Enum",
    "Function",
    "TypeAlias",
    "Variable"
  ],
  "categorizeByGroup": true,
  "searchInComments": true,
  "searchInDocuments": true,
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "hostedBaseUrl": "https://saiyan9001.github.io/aura-video-studio/api/typescript/",
  "customCss": "./typedoc-theme.css",
  "hideGenerator": false
}
